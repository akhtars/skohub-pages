@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix classification: <https://www.dartmouth.edu/library/catmet/old-dartmouth-schedules/classification> .
@prefix dartclass: <https://www.dartmouth.edu/library/catmet/old-dartmouth-schedules/class> .
@prefix dartsubclass: <https://www.dartmouth.edu/library/catmet/old-dartmouth-schedules/subclass> .

classification:old-dartmouth a skos:ConceptScheme ;
	skos:hasTopConcept dartclass:h ;
	skos:prefLabel "Old Dartmouth Classification System"@en .

dartclass:h a skos:Concept ;
	skos:inScheme classification:old-dartmouth ;
	skos:narrower dartsubclass:h ;
	skos:notation "Class H" ;
	skos:prefLabel "History"@en ;
	skos:scopeNote "Lives of sovereigns are classified in History.  Cutter from biographee and add letter of biographer.  Do not use z in the call no.  Do not change books already cataloged with z.  There will be a few cases, such as another book or another edition by same biographer, where z has been used on the earlier work and may be used on the new book to keep the two books together.  Feb. 1942." .

